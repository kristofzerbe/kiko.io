<%# if (page.photograph) { %>
  <% //TODO: Refactor to _source/archives/index.md
    page.photograph = {
      file: "$22-05-Malta-7531.jpg",
      name: "Suspended Window"
    }
  %>
  <%- partial('_partial/photograph') %>
<%# } %>

<% 
let currentClass = "";
if (page.year) { currentClass = "skip-right"; }
%>

<%- partial('_partial/nav-index-archives', { currentClass: currentClass }) %>

<% if (page.year) { %>
  <h1 class="page-title">
    <small>Articles</small>
  <% if (page.month) { %>
    <em class="date"></em><%= moment().month(page.month).subtract(1, "month").format('MMMM') %> <%= page.year %>
  <% } else { %>
    <em class="date"></em><%= page.year %>
  <% } %>
  </h1>
<% } else { %>
  <h1 class="page-title">Articles</h1>
<% } %>

<%- partial('_partial/archive-navigation') %>
<%- partial('_partial/archive-item', { pagination: config.archive, index: true }) %>