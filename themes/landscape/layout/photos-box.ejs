<% if (page.photograph){ %>
  <%- partial('_partial/photograph') %>
<% } %>

<%- partial('_partial/nav-index-photos', { currentClass: "skip-right" }) %>

<h1 class="page-title">
  <small>Photos</small>
  <em class="box"></em><%= page.title %>
</h1>

<div class="page-content">  
  <p>These <strong><%= page.items.length %></strong> photos were taken during the period <%= page.periodString %></p>
  <hr>
</div>

<hr class="divider" />

<div class="cardset">
  <div class="cards grid">
    <% for(var i=0; i < page.items.length; i++) { %>
      <%- partial('_partial/photo-item', { item: page.items[i] }) %>
    <% } %>
  </div>
</div>