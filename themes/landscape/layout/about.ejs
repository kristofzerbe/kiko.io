<% if (page.photograph){ %>
  <%- partial('_partial/photograph') %>
<% } %>

<h1 class="page-title"><%= page.title %></h1>
<% if (page.subtitle) { %>
<h2 class="page-subtitle"><%= page.subtitle %></h2>
<% } %>

<div class="page-content">
  <%- page.content %>
</div>

<script>
  //--- set contact link
  var elm = document.getElementById("contact-Link");
  elm.onclick = function(e) {
    e.preventDefault();
    dialog.contact();
  }
</script>