
<article id="note-<%= note.slug %>" data-key="<%= note.key %>" class="article note h-entry">

  <div class="article-meta">
    <time datetime="<%= date_xml(note.date) %>"><%= date(note.date, 'DD MMM YYYY') %></time>
  </div>

  <div class="article-inner">

    <header class="article-header">
      <%- partial('post/title', { post: note, class_name: 'article-title p-name', show_link: true, index: true }) %>    
    </header>
  
    <div class="article-entry">
      <%- note.excerpt %>
      <% if (note.more && theme.excerpt_link){ %>
        <p class="article-more-link">
          <a href="<%- url_for(note.link) %>"><%= theme.excerpt_link %></a>
        </p>
      <% } %>
    </div>

    <footer class="article-footer">      
      <%- partial('post/syndication', { post: note, class_name: 'article-syndication' }) %>
      <%- partial('post/permalink', { post: note, class_name: 'article-permalink' }) %>
    </footer>
    
  </div>

</article>