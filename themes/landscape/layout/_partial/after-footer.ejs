<!-- MAIN-SCRIPT (THEME.LANDSCAPE.SOURCE.JS) -->
<%- js('js/script') %>

<script>
  //BACK-TO-TOP
  addBackToTop({
    diameter: 30
  })

// AUTOTYPING
if (getCookie("typing")) {
  $("#title-wrap").text("<%= config.title %>");
} else {
  new AutoTyping({
    id: 'title-wrap',
    typeText: ['<%= config.title %>'],
    textColor: '#fff',
    typeSpeed: 100,
    typeDelay: 1000,
    showCursor: false,
    delete: false,
    typeInfinity: false
  }).init();
  setCookie("typing", true, 1)
}
</script>
