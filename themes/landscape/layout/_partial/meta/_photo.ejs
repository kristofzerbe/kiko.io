{
  "@type": "ImageObject",
  "@id": "<%= config.url %>#photo/<%= meta.photograph.file %>",
  "caption": "<%= meta.photograph.name %>",
  "url": "<%= config.url %><%= meta.photograph.url %>",
  "contentUrl": "<%= config.url %><%= meta.photograph.url %>",
  "discussionUrl": "<%= config.url %>/<%= config.photo_dir %>/<%= meta.photograph.file.replace(".jpg", "") %>",
  "license": "https://creativecommons.org/licenses/by-sa/4.0/",
  "acquireLicensePage": "<%= config.url %>/photos",
  "creditText": "<%= config.author %>",
  "copyrightNotice": "<%= config.author %> (CC BY-SA 4.0)",
  "creator": {
    "@id": "<%= config.url %>/#person"
  }
}