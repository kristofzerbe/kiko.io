{
  "@type": "Article",
  "@id": "<%= url.replace("index.html", "") %>#article",
  "mainEntityOfPage": {
    "@id": "<%= url.replace("index.html", "") %>"
  },
  "headline": "<%= meta.title %>",
  "description": "<%= meta.description %>",
  "datePublished": "<%= moment(page.date).utc().format() %>",
  <% if (page.updated > page.date) { %> 
    "dateModified": "<%= moment(page.updated).utc().format() %>",
  <% } else { %>
    "dateModified": "<%= moment(page.date).utc().format() %>",  
  <% } %>
  "image": {
    "@id": "<%= config.url %>#image/<%= meta.slug %>"
  },
  "author": {
    "@id": "<%= config.url %>/#person"
  },
  "publisher": {
    "@id": "<%= config.url %>/#organization"
  }
}