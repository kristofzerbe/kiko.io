<div class="card photo" data-status="<%- item.status %>">
  <% if (item.article){ %>
  <a class="status <%- item.status %> <%- item.article.type %>" href="<%- item.article.url %>"><%- item.article.title %></a>
  <% } else { %>
  <span class="status <%- item.status %>">unused</span>
  <% } %>
  <a class="spotlight card-img" 
       data-title="<%- item.name %>"
       data-description="<%- item.key %>&nbsp;&bull;&nbsp;<%- item.status %>"
       data-src="<%- item.photo %>">
    <img src="<%- item.path %>" alt="<%- item.name %>" />
  </a>
  <span class="pre-title"><%- item.key %></span>
  <h3><%- item.name %></h3>
  <% if (item.link) { %>
  <a class="icon-link" href="<%- item.link %>">
    <img src="/images/500px_full.svg" alt="see on 500px" style="height" />
  </a>
  <% } %>
</div>