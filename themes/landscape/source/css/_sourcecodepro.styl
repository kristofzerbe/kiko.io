
@font-face
    font-family: "Source Code Pro"
    font-weight: 300 400
    font-style: normal
    font-stretch: normal
    font-display: swap
    src: url(font-path + "/sourcecodepro/WOFF2/TTF/SourceCodePro-Regular.ttf.woff2") format("woff2"),
         url(font-path + "/sourcecodepro/TTF/SourceCodePro-Regular.ttf") format("truetype")

@font-face
    font-family: "Source Code Pro"
    font-weight: 400
    font-style: italic
    font-stretch: normal
    font-display: swap
    src: url(font-path + "/sourcecodepro/WOFF2/TTF/SourceCodePro-It.ttf.woff2") format("woff2"),
         url(font-path + "/sourcecodepro/TTF/SourceCodePro-It.ttf") format("truetype")

@font-face
    font-family: "Source Code Pro"
    font-weight: 600 700
    font-style: normal
    font-stretch: normal
    font-display: swap
    src: url(font-path + "/sourcecodepro/WOFF2/TTF/SourceCodePro-Bold.ttf.woff2") format("woff2"),
         url(font-path + "/sourcecodepro/TTF/SourceCodePro-Bold.ttf") format("truetype")

@font-face
    font-family: "Source Code Pro"
    font-weight: 600 700
    font-style: italic
    font-stretch: normal
    font-display: swap
    src: url(font-path + "/sourcecodepro/WOFF2/TTF/SourceCodePro-BoldIt.ttf.woff2") format("woff2"),
         url(font-path + "/sourcecodepro/TTF/SourceCodePro-BoldIt.ttf") format("truetype")

