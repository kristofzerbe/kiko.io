<html>
    <head>
        <style>
            body {
                padding: 0;
                margin: 0;
                font-family: -apple-system,BlinkMacSystemFont,"Open Sans","Roboto","Oxygen","Ubuntu","Cantarell","Fira Sans","Droid Sans","Helvetica Neue",sans-serif;;
            }            
            .wrap {
                position: relative;
                width: 1200px;
                height: 630px;            
            }
            .wrap img#photo {
                width: 100%;
                max-height: 630px;
                object-fit: cover;
            }
            .container {
                position: absolute;
                width: 100%; 
                height: 100%;
                top:0; 
                left:0;
                z-index: 1; 
                box-sizing: border-box;
                color: #fff;
                display: grid;
                grid-template-rows: 120px 60px auto;
                grid-template-columns: auto 200px;
                grid-template-areas:
                    '. logo'
                    '. domain'
                    'title title';
            }
            .container img#logo {
                grid-area: logo;
                width: 64px;
                height: 64px;                
                justify-self: center;
                align-self: end;                
                box-shadow: -5px 5px 15px 5px rgba(0,0,0,0.25);
                border: 1px rgba(0,0,0,0.4) solid;
            }   
            label {
                grid-area: domain;
                justify-self: center;
                text-transform: uppercase;
                margin-top: 10px;
                font-weight: bold;
                text-shadow: -5px 5px 15px rgb(0 0 0 / 60%);
            }
            .container section#title {
                grid-area: title;
                justify-self: end;
                align-self: end;
                width: 100%;
                backdrop-filter: blur(20px) brightness(90%);
                padding-bottom: 40px;
            }
            .container small {
                color: rgba(255, 255, 255, 0.75);
                background-color: rgba(255, 255, 255, 0.25);
                font-size: 18px;
                font-weight: bold;
                padding: 5px 10px;
                margin-left: 40px;
                margin-top: 25px;
                display: inline-block;
            }
            .container h1,
            .container h2 {
                margin: 0;
                padding-left: 40px;
                padding-right: 40px;
                text-shadow: -5px 5px 15px rgba(0, 0, 0, 0.35);
            }
            .container h1 {
                font-size: 58px;
                padding-top: 20px;                
            }
            .container h2 {
                font-size: 32px;            
                font-weight: normal;
                padding-top: 20px;
            }
        </style>
    </head>
    <body>
        <div class="wrap">
            <img id="photo" src="{{photo}}">
            <div class="container">
                <img id="logo" src="data:image/png;base64," alt="kiko.io">
                <label>kiko.io</label>
                <section id="title">
                    {{#each categories}}
                    <small>{{this}}</small>
                    {{/each}}
                    <h1>{{title}}</h1>
                    {{#if subtitle}}
                    <h2>{{subtitle}}</h2>
                    {{/if}}
                </section>
            </div>
        </div>
    </body>
</html>