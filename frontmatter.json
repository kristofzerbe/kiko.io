{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "other",
  "frontMatter.framework.startCommand": "hexo server",
  "frontMatter.preview.host": "http://localhost:4000/",
  "frontMatter.templates.folder": "scaffolds",
  "frontMatter.content.publicFolder": "",
  "frontMatter.content.fmHighlight": true,
  "frontMatter.content.wysiwyg": false,
  "frontMatter.content.pageFolders": [
    {
      "title": "Posts",
      "path": "[[workspace]]/source/_posts"
    }
  ],
  "frontMatter.content.sorting": [
    {
      "title": "PublishDate (asc)",
      "name": "date",
      "order": "asc",
      "type": "date"
    },
    {
      "title": "PublishDate (desc)",
      "name": "date",
      "order": "desc",
      "type": "date"
    }
  ],
  "frontMatter.content.draftField": {
    "name": "hidden",
    "type": "boolean"
  },
  "frontMatter.content.placeholders": [
    {
      "id": "sm",
      "value": "/static/images/social-media/{{slug}}.png"
    }
  ],
  "frontMatter.taxonomy.dateFormat": "yyyy-MM-DD",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "Slug",
          "name": "slug",
          "type": "string",
          "single": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Subitle",
          "name": "subtitle",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "isPublishDate": true,
          "isModifiedDate": true
        },
        {
          "title": "Photograph",
          "name": "photograph",
          "type": "fields",
          "fields": [
            {
              "title": "File",
              "name": "file",
              "type": "string",
              "single": true
            },
            {
              "title": "Name",
              "name": "name",
              "type": "string",
              "single": true
            },
            {
              "title": "Link",
              "name": "link",
              "type": "string"
            },
            {
              "title": "Social Media",
              "name": "socialmedia",
              "type": "image",
              "isPreviewImage": true
            }
          ]
        },
        {
          "title": "Series",
          "name": "series",
          "type": "choice",
          "choices": [
            "Golem",
            "Lightroom Presets",
            "Discoveries",
            "Great Finds",
            "Step By Step",
            "IndieWeb",
            "A New Blog"
          ]
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Hidden",
          "name": "hidden",
          "type": "draft"
        }
      ]
    }
  ],
  "frontMatter.taxonomy.tags": [
    "Browser",
    "CSS",
    "Collection",
    "Imaging",
    "Lightroom",
    "Mail",
    "MediaQuery",
    "Node.js",
    "PWA",
    "Plugin",
    "PowerShell",
    "Presets",
    "Publishing",
    "Resource",
    "SPA",
    "SVG",
    "Share",
    "Stylus",
    "T4",
    "TFS/DevOps",
    "Templating",
    "Theming",
    "Trello",
    "Tutorial",
    "VS Code",
    "Versioning",
    "Visual Studio",
    "WebAPI",
    "Windows",
    "Workflow",
    "jQuery"
  ],
  "frontMatter.taxonomy.categories": [
    "Photo-Editing",
    "Tools",
    "UI/UX",
    "SQL",
    "Misc",
    "JavaScript",
    ".NET"
  ]
}