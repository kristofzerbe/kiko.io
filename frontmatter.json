{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.site.baseURL": "https//kiko.io",
  "frontMatter.framework.id": "hexo",
  "frontMatter.framework.startCommand": "hexo server",
  "frontMatter.preview.host": "http://localhost:4000/",
  "frontMatter.preview.pathName": "post",
  "frontMatter.templates.enabled": true,
  "frontMatter.templates.folder": "./scaffolds",
  "frontMatter.content.publicFolder": {
    "path": "static",
    "relative": true
  },
  "frontMatter.content.fmHighlight": true,
  "frontMatter.content.wysiwyg": true,
  "frontMatter.content.pageFolders": [
    {
      "title": "Posts",
      "path": "[[workspace]]/source/_posts",
      "previewPath": "post"
    },
    {
      "title": "Drafts",
      "path": "[[workspace]]/source/_drafts"
    }
  ],
  "frontMatter.content.sorting": [
    {
      "title": "Updated (asc)",
      "name": "updated",
      "order": "asc",
      "type": "date"
    },
    {
      "title": "Updated (desc)",
      "name": "updated",
      "order": "desc",
      "type": "date"
    }
  ],
  "frontMatter.content.draftField": {
    "name": "hidden",
    "type": "boolean"
  },
  "frontMatter.content.placeholders": [
    {
      "id": "sm",
      "value": "/static/images/social-media/{{slug}}.png"
    }
  ],
  "frontMatter.taxonomy.dateFormat": "yyyy-MM-DD",
  "frontMatter.taxonomy.indentArrays": true,
  "frontMatter.taxonomy.noPropertyValueQuotes": [
    "series"
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "Slug",
          "name": "slug",
          "type": "string",
          "single": true
        },
        {
          "title": "Alias",
          "name": "alias",
          "type": "string",
          "single": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Subtitle",
          "name": "subtitle",
          "type": "string"
        },
        {
          "title": "Date",
          "name": "date",
          "type": "datetime",
          "isPublishDate": true
        },
        {
          "title": "Updated",
          "name": "updated",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "Photograph",
          "name": "photograph",
          "type": "fields",
          "fields": [
            {
              "title": "File",
              "name": "file",
              "type": "string",
              "single": true
            },
            {
              "title": "Name",
              "name": "name",
              "type": "string",
              "single": true
            },
            {
              "title": "Link",
              "name": "link",
              "type": "string"
            },
            {
              "title": "Social Media",
              "name": "socialmedia",
              "type": "image",
              "isPreviewImage": true
            }
          ]
        },
        {
          "title": "Series",
          "name": "series",
          "type": "taxonomy",
          "taxonomyId": "series"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Related",
          "name": "related",
          "type": "list"
        },
        {
          "title": "Syndication",
          "name": "syndication",
          "type": "block",
          "fieldGroup": [
            "syndication"
          ]
        },
        {
          "title": "Hidden",
          "name": "hidden",
          "type": "draft"
        }
      ]
    }
  ],
  "frontMatter.taxonomy.fieldGroups": [
    {
      "id": "syndication",
      "labelField": "host",
      "fields": [
        {
          "title": "Host",
          "name": "host",
          "type": "choice",
          "choices": [
            "Mastodon"
          ],
          "single": true
        },
        {
          "title": "URL",
          "name": "url",
          "type": "string",
          "single": true
        }
      ]
    }
  ],
  "frontMatter.taxonomy.tags": [
    "Browser",
    "CSS",
    "Collection",
    "Imaging",
    "Lightroom",
    "Mail",
    "MediaQuery",
    "Node.js",
    "PWA",
    "Plugin",
    "PowerShell",
    "Presets",
    "Publishing",
    "Resource",
    "SPA",
    "SVG",
    "Share",
    "Stylus",
    "T4",
    "TFS/DevOps",
    "Templating",
    "Theming",
    "Trello",
    "Tutorial",
    "VS Code",
    "Versioning",
    "Visual Studio",
    "WebAPI",
    "Windows",
    "Workflow",
    "jQuery",
    "SVWW"
  ],
  "frontMatter.taxonomy.categories": [
    "Photo-Editing",
    "Tools",
    "UI/UX",
    "SQL",
    "Misc",
    "JavaScript",
    ".NET",
    "Football"
  ],
  "frontMatter.taxonomy.customTaxonomy": [
    {
      "id": "series",
      "options": [
        "A New Blog",
        "Discoveries",
        "Great Finds",
        "Golem",
        "IndieWeb",
        "Lightroom Presets",
        "Step By Step",
        "SV Wehen Wiesbaden"
      ]
    }
  ]
}