{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.dashboard.content.card.fields.title": "title",
  "frontMatter.dashboard.content.card.fields.description": "subtitle",
  "frontMatter.dashboard.content.pagination": 24,
  "frontMatter.site.baseURL": "https//kiko.io",
  "frontMatter.framework.id": "hexo",
  "frontMatter.framework.startCommand": "hexo server",
  "frontMatter.preview.host": "http://localhost:4000/",
  "frontMatter.preview.pathName": "post",
  "frontMatter.content.publicFolder": {
    "path": "./static/photos/normal",
    "relative": false
  },
  "frontMatter.content.fmHighlight": true,
  "frontMatter.content.wysiwyg": true,
  "frontMatter.content.pageFolders": [
    {
      "title": "Posts",
      "path": "[[workspace]]/source/_posts",
      "previewPath": "post"
    },
    {
      "title": "Drafts",
      "path": "[[workspace]]/source/_drafts"
    }
  ],
  "frontMatter.content.sorting": [
    {
      "title": "Updated (asc)",
      "name": "updated",
      "order": "asc",
      "type": "date"
    },
    {
      "title": "Updated (desc)",
      "name": "updated",
      "order": "desc",
      "type": "date"
    }
  ],
  "frontMatter.content.placeholders": [
    {
      "id": "sm",
      "value": "/static/images/social-media/{{slug}}.jpg"
    }
  ],
  "frontMatter.taxonomy.dateFormat": "yyyy-MM-DD",
  "frontMatter.taxonomy.indentArrays": true,
  "frontMatter.taxonomy.noPropertyValueQuotes": [
    "series"
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "Slug",
          "name": "slug",
          "type": "string",
          "single": true
        },
        {
          "title": "Alias",
          "name": "alias",
          "type": "string",
          "single": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Subtitle",
          "name": "subtitle",
          "type": "string"
        },
        {
          "title": "Date",
          "name": "date",
          "type": "datetime",
          "isPublishDate": true
        },
        {
          "title": "Updated",
          "name": "updated",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "Photograph",
          "name": "photograph",
          "type": "fields",
          "fields": [
            {
              "title": "File",
              "name": "file",
              "single": true,
              "type": "image",
              "isPreviewImage": true
            },
            {
              "title": "Name",
              "name": "name",
              "type": "string",
              "single": true
            },
            {
              "title": "Link",
              "name": "link",
              "type": "string"
            },
            {
              "title": "Social Media",
              "name": "socialmedia",
              "type": "image",
              "default": "{{sm}}"
            }
          ]
        },
        {
          "title": "Series",
          "name": "series",
          "type": "taxonomy",
          "taxonomyId": "series"
        },
        {
          "title": "Project",
          "name": "project",
          "type": "taxonomy",
          "taxonomyId": "project"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Related",
          "name": "related",
          "type": "list"
        },
        {
          "title": "Syndication",
          "name": "syndication",
          "type": "block",
          "fieldGroup": [
            "syndication"
          ]
        },
        {
          "title": "Hidden",
          "name": "hidden",
          "type": "boolean"
        }
      ]
    }
  ],
  "frontMatter.taxonomy.fieldGroups": [
    {
      "id": "syndication",
      "labelField": "host",
      "fields": [
        {
          "title": "Host",
          "name": "host",
          "type": "choice",
          "choices": [
            "Mastodon"
          ],
          "single": true
        },
        {
          "title": "URL",
          "name": "url",
          "type": "string",
          "single": true
        }
      ]
    }
  ],
  "frontMatter.taxonomy.customTaxonomy": [
    {
      "id": "series",
      "options": [
        "A New Blog",
        "Discoveries",
        "Great Finds",
        "Golem",
        "IndieWeb",
        "Lightroom Presets",
        "Step By Step",
        "SV Wehen Wiesbaden",
        "Thomas Garden",
        "New Photos"
      ]
    },
    {
      "id": "project",
      "options": [
        "Hexo Console Webmention",
        "Hexo Generator Anything",
        "Hexo Tag Plugins",
        "Social Media Image Generator"
      ]
    }
  ]
}